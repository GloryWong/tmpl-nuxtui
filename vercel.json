{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "statusCode": 200
    }
  ],

  "headers": [
    {
      "source": "/manifest.webmanifest",
      "headers": [
        {
          "Content-Type": "application/manifest+json"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "Cache-Control": "max-age=31536000, immutable"
        }
      ]
    }
  ]
}

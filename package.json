{
  "name": "tmpl-nuxtui",
  "type": "module",
  "version": "0.1.6",
  "private": true,
  "description": "Ready-to-use Nuxt UI template",
  "author": {
    "name": "Glory Wong",
    "email": "glorywong1001@gmail.com",
    "url": "https://glorywong.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GloryWong/tmpl-nuxtui.git"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "generate-pwa-assets": "pwa-assets-generator",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "commit": "git-cz",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@gloxy/mini-logger": "^0.0.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.12.1",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@iconify-json/line-md": "^1.1.37",
    "@morev/vue-transitions": "^3.0.2",
    "@nuxt/devtools": "latest",
    "@nuxt/test-utils": "^3.12.0",
    "@nuxt/ui": "^2.15.1",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vite-pwa/nuxt": "^0.6.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vueuse/nuxt": "^10.9.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-perfectionist": "^2.8.0",
    "happy-dom": "^13.10.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nuxt": "^3.11.2",
    "nuxt-security": "1.2.2",
    "type-fest": "^4.15.0",
    "vitest": "^1.4.0",
    "vitest-environment-nuxt": "^1.0.0",
    "vitest-fetch-mock": "^0.2.2"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "overrides": {
    "vue": "latest"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  }
}
